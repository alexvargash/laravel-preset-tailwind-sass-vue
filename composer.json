    "keywords": ["laravel", "laravel preset", "tailwindcss", "sass", "vue", "vue-router"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Vargas",
            "email": "alexvargas.dev@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
        "psr-4": {
            "Alexvargash\\LaravelPreset\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": ["Alexvargash\\LaravelPreset\\TailwindSassVueServiceProvider"]
        }
    }
}
